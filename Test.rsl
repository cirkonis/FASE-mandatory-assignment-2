NETTest
	
scheme test = 
  extend NETTest with 
  class 
    value 
      nett : Net = (<..>,<..>), 
      stID : Text = "ab", 
      cap : Nat = 3 
    	 
    test_case 
      [t1]
        1 + 1, 
      nett, 
      [t2]
        insertStop(stID, cap, nett),
	      insertStop("bc", 2,insertStop(stID, cap, nett)),

      [t3]
	      getStop("ab",<.("ab",3),("bc",2),("cd",5).>),

      [t4]
	      addConnection("ab", "bc", 1, 4, 2, insertStop("bc", 2,insertStop(stID, cap, nett)))
  end